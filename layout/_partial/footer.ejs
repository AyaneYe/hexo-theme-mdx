<footer class="footer">
	<div class="footerUpperLayer">
		<div class="title">
			<a href="/"><%-config.title%></a>
		</div>
		<div class="detail">
			<div class="text">
				<%-theme.footer.text%>
				<% if (theme.footer.contact) { %>
					&nbsp;&nbsp;
					<div class="contact">
						<% for(element in theme.footer.contact){%>
							<%const str = theme.footer.contact[element].split(" || ")%>
							<a href="<%= url_for(str[0]) %>">
								<i class="<%=str[1]%>"></i>	
							</a>
						<%}%>
					</div>
				<% } %>
			</div>
			<div class="copyrights">
				<%- _p('footer.copyrights',`${theme.footer.since || (new Date()).getFullYear()} - ${(new Date()).getFullYear()} ${config.author}`) %>
			</div>
		</div>
	</div>
	<hr />
	<div class="footerLowerLayer">
		<div class="left">
			<% if (theme.footer.hexo) { %>
				<a href="https://hexo.io" target="_blank"><%- __('footer.hexo') %></a>
			<% } %>
			<% if (theme.footer.hexo && theme.footer.theme) { %>
				&nbsp;&&nbsp;
			<% } %>
			<% if (theme.footer.theme) { %>
				<a href="https://" target="_blank"><%- __('footer.theme') %></a>
			<% } %>
		</div>
		<div class="right">
			<% if (theme.footer.miit_icp) { %>
				<a href="https://beian.miit.gov.cn/"><%= theme.footer.miit_icp %></a>
			<% } %>
			<% if (theme.footer.miit_icp && theme.footer.moe_icp) { %>
				&nbsp;/&nbsp;
			<% } %>
			<% if (theme.footer.moe_icp) { %>
				<a href="https://icp.gov.moe/?keyword=<%= theme.footer.moe_icp %>">萌ICP备<%= theme.footer.moe_icp %>号</a>
			<% } %>
		</div>
	</div>
</footer>